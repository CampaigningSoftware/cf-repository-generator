{
    "name": "campaigningsoftware/cf-repository-generator",
    "description": "Generates required repositories, factories and models to access a new Contentful Content Type in Laravel",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "contentful/laravel": "^9.0",
        "fakerphp/faker": "^1.23",
        "davidbadura/faker-markdown-generator": "^1.1",
        "nesbot/carbon": "^3.8",
        "laravel/framework": "^11.0"
    },
    "authors": [
        {
            "name": "Stefan Schindler",
            "email": "stefan.schindler@cambuildr.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CampaigningSoftware\\CfRepositoryGenerator\\": "src"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CampaigningSoftware\\CfRepositoryGenerator\\CfRepositoryGeneratorServiceProvider"
            ]
        }
    }
}
